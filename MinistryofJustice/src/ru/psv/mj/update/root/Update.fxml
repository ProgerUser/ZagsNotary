<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">

	<children>
      <VBox fx:id="Box" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="400.0" spacing="5.0">
         <children>
      		<Text nodeOrientation="LEFT_TO_RIGHT" strokeType="OUTSIDE" strokeWidth="0.0" text="Обновить приложение ?">
      			<font>
      				<Font size="14.0" />
      			</font>
               <VBox.margin>
                  <Insets left="100.0" />
               </VBox.margin>
      		</Text>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
            		<Button mnemonicParsing="false" onAction="#Yes" prefHeight="25.0" prefWidth="83.0" text="Да" />
            		<Button mnemonicParsing="false" onAction="#Cencel" prefHeight="25.0" prefWidth="83.0" text="Нет" />
              </buttons>
            </ButtonBar>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <ProgressIndicator fx:id="PI" maxHeight="50.0" maxWidth="50.0" visible="false" />
	</children>

</StackPane>
