<VBox fx:id="VBOX_ROOT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="500.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.261">
   <children>
      <TitledPane fx:id="T_F" alignment="CENTER" collapsible="false" style="-fx-border-color: black;" text="IF_TEXT_FIELD">
         <content>
            <VBox>
               <children>
                  <TextField fx:id="IF_TEXT_FIELD" />
               </children>
            </VBox>
         </content>
      </TitledPane>
      <TitledPane fx:id="T_A" alignment="CENTER" collapsible="false" style="-fx-border-color: black;" text="IF_TEXT_AREA">
         <content>
            <VBox>
               <children>
                  <TextArea fx:id="IF_TEXT_AREA" />
               </children>
            </VBox>
         </content>
      </TitledPane>
      <TitledPane fx:id="L_F" alignment="CENTER" collapsible="false" style="-fx-border-color: black;" text="IF_LIST_FIELD_T">
         <content>
            <VBox>
               <children>
                  <ToolBar>
                    <items>
                      <Button fx:id="IF_LIST_FIELD_B" mnemonicParsing="false" onAction="#IF_LIST_FIELD_B">
                           <font>
                              <Font size="10.0" />
                           </font>
                           <graphic>
                              <FontAwesomeIconView fontSmoothingType="LCD" glyphName="LIST_OL" size="18" />
                           </graphic>
                           <tooltip>
                              <Tooltip text="Открыть список" />
                           </tooltip>
                        </Button>
                    </items>
                     <VBox.margin>
                        <Insets bottom="5.0" left="-10.0" right="-10.0" top="-10.0" />
                     </VBox.margin>
                  </ToolBar>
                  <GridPane>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="500.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Наименование" />
                        <TextField fx:id="IF_LIST_FIELD_NAME" editable="false" GridPane.rowIndex="1" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Код" GridPane.rowIndex="2" />
                        <TextField fx:id="IF_LIST_FIELD_ID" editable="false" GridPane.rowIndex="3" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
         </content>
      </TitledPane>
      <TitledPane fx:id="D_F" alignment="CENTER" collapsible="false" style="-fx-border-color: black;" text="IF_DATE_FIELD">
         <content>
            <VBox>
               <children>
                  <DatePicker fx:id="IF_DATE_FIELD" prefWidth="120.0" />
               </children>
            </VBox>
         </content>
      </TitledPane>
      <ButtonBar>
        <buttons>
          <Button fx:id="OK" mnemonicParsing="false" onAction="#OK" text="Сохранить" />
