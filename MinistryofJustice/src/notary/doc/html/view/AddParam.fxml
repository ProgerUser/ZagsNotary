<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TreeTableView fx:id="param" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="500.0" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
        <columns>
          <TreeTableColumn fx:id="id" prefWidth="75.0" text="ID" />
          <TreeTableColumn fx:id="name" prefWidth="250.0" text="Название" />
            <TreeTableColumn fx:id="req" prefWidth="100.0" text="Обяз." />
        </columns>
      </TreeTableView>
      <ButtonBar>
        <buttons>
          <Button mnemonicParsing="false" onAction="#ok" text="Добавить" />
            <Button layoutX="20.0" layoutY="10.0" mnemonicParsing="false" onAction="#cenc" text="Отмена" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
