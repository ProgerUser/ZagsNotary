<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox fx:id="root" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.271" xmlns:fx="http://javafx.com/fxml/1">
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
	<children>
      <Button layoutX="15.0" layoutY="15.0" mnemonicParsing="false" onAction="#reload" text="Load html" />
      <Button mnemonicParsing="false" onAction="#editor" text="editor" />
      <Separator prefWidth="200.0" />
		<WebView fx:id="webView" minHeight="600.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="900.0" VBox.vgrow="ALWAYS" />
	</children>
</VBox>
