<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView fx:id="ZAGS" prefWidth="575.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="ZAGS_ID" prefWidth="46.0" text="Номер" />
          <TableColumn fx:id="ZAGS_OTD" prefWidth="81.0" text="Отделение" />
            <TableColumn fx:id="ZAGS_NAME" prefWidth="276.0" text="Наименование" />
            <TableColumn fx:id="ZAGS_RUK" minWidth="0.0" prefWidth="169.0" text="Руководитель" />
        </columns>
      </TableView>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <ToolBar BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#Add" text="Добавить" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#Edit" text="Редактировать" />
            <Button layoutX="83.0" layoutY="10.0" mnemonicParsing="false" onAction="#Delete" text="Удалить" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
